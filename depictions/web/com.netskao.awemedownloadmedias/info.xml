  <id>com.netskao.awemedownloadmedias</id>
  <name>抖音下载 AwemeDownloadMedia</name>
  <version>1.1-8</version>
  <compatibility>
    <firmware>
      <miniOS>0.3</miniOS>
    </firmware>
  </compatibility>
