  <name>小红书图层</name>
  <version>1.1.2</version>
  <compatibility>
    <firmware>
      <miniOS>0.3</miniOS>
    </firmware>
  </compatibility>
  <dependencies>
    <package>mobilesubstrate (&gt;= 0.9.5000)</package>
